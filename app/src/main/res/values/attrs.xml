        <attr name="bottom_left_bottomMargin" format="integer"/>
        <!--bottom-right文字-->
        <attr name="bottom_right_text" format="string"/>
        <attr name="bottom_right_textSize" format="integer"/>
        <attr name="bottom_right_textColor" format="color"/>
        <attr name="bottom_right_leftMargin" format="integer"/>
        <attr name="bottom_right_topMargin" format="integer"/>
        <attr name="bottom_right_rightMargin" format="integer"/>
        <attr name="bottom_right_bottomMargin" format="integer"/>

    </declare-styleable>
</resources>